{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "scripts": {
    "prepare": "concurrently \"npm run typings\" \"npm run postinstall\" \"npm run webpack\" ",
    "devserver": "concurrently \"webpack-dev-server --inline --progress --colors\" \"open http://localhost:8080\"",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "license": "ISC",
  "dependencies": {
    "angular2": "2.0.0-beta.8",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.5.15"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^0.15.10",
    "concurrently": "^2.0.0",
    "lite-server": "^2.1.0",
    "typescript": "^1.8.2",
    "typings": "^0.6.8",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
